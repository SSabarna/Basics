#include<stdio.h>
int main()
{
    int xl,x2,y1,y2;
    printf("Enter xl\n");
    scanf("%d",&xl);
    printf("Enter yl\n");
    scanf("%d",&y1);
    printf("Enter x2\n");
    scanf("%d",&x2);
    printf("Enter y2\n");
    scanf("%d",&y2);
    printf("Binoy's house is located at (%.1f , %.1f)",(float)(xl+x2)/2,(float)(y1+y2)/2);
    return 0;
}
