#include <stdio.h>
int main()
{
    int i=0;
    char s[99],ch;
    scanf("%s",s);
    while(s[i]!='\0')
    {
    	ch=s[i];
    	if(ch>='a' && ch<='z')
    	{
    		s[i]=s[i]-32;
    	}
    	if(ch>='A' && ch<='Z')
    	{
    		s[i]=s[i]+32;
    	}
    	i++;
    }
    printf("%s",s);
    return 0;
}
