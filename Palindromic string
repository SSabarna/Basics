#include <stdio.h>
#include <string.h>
int main()
{
    int i,flag=0;
    char s[99];
    scanf("%s",s);
    int length=strlen(s);
    for(i=0;i<length;i++)
    {
    	if(s[i]!=s[length-i-1])
    	{
    		flag=1;
    		break;
    	}
    }
    if(flag)
    {
    	printf("NO");
    }
    else
    {
    	printf("YES");
    }
    return 0;
}
